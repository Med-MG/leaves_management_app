RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?congeapp.com$
RewriteCond %{REQUEST_URI} !^/Login/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /Login/$1
RewriteCond %{HTTP_HOST} ^(www.)?congeapp.com$
RewriteRule ^(/)?$ Login/index.php [L]

RedirectMatch 301 ^/resources/$ http://congeapp.com/public